{
  "schema_version": "aal.gametheory.vector.v1",
  "type": "object",
  "required": ["schema_version", "vector_id", "game", "expected", "notes", "provenance"],
  "properties": {
    "schema_version": {"type": "string"},
    "vector_id": {"type": "string"},
    "game": {
      "type": "object",
      "required": ["players", "strategies", "payoffs", "type"],
      "properties": {
        "players": {"type": "integer"},
        "strategies": {"type": "array", "items": {"type": "array", "items": {"type": "string"}}},
        "payoffs": {"type": "array", "items": {"type": "array", "items": {"type": "array", "items": {"type": "number"}}}},
        "type": {"type": "string", "enum": ["normal_form", "repeated", "bayesian_stub"]}
      }
    },
    "expected": {
      "type": "object",
      "required": ["equilibria", "dominant_strategies"],
      "properties": {
        "equilibria": {"type": "array", "items": {"type": "object"}},
        "dominant_strategies": {"type": "array", "items": {"type": "string"}}
      }
    },
    "notes": {"type": "string"},
    "provenance": {"type": "object"}
  }
}
